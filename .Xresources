!--- Sets ---!
!URxvt.geometry: 400x400
URxvt.scrollBar: False
Xcursor.theme: BreezeX-Dark
!Xcursor.size: 30
!URxvt.fading: 65
 
!--- Font ---!
!Xft.dpi: 82
!URxvt.lineSpace: 1
!URxvt.letterSpaces: 1
!URxvt.font: xft:Hack Nerd Font:size=10,xft:DejaVuSansM Nerd Font:style=Regular:size=10,xft:FiraCode Nerd Font:style=Regular:size=9
!*.font: xft:Hack Nerd Font:style=Regular:size=10,xft:DejaVuSansM Nerd Font:style=Regular:size=10,xft:FiraCode Nerd Font:style=Regular:size=9
*.font: xft:JetBrains Mono Nerd Font:size=12,xft:Hack Nerd Font:style=Regular:size=10,xft:FiraCode Nerd Font:style=Regular:size=9

!--- Extensions ---!
URxvt.perl-ext-common: default,matcher,selection-to-clipboard
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste



